#search-bar
  height: 55px
  border-bottom: 1px solid #ccc
  background-color: white
  background-color: #f1f1f1
  .wrapper
    padding-top: 0
    .search
      input[type="text"]
        font-size: 16px
        color: #434343
        font-size: 14px
        margin-left: 55px
        border: 1px solid #aaa
        border-radius: 0
        padding: 5px
        padding-right: 20px
        width: 300px
        position: relative
        top: -8px
    .aoq
      background: url("//ssl.gstatic.com/ui/v1/disclosure/light-grey-disclosure-arrow-down.png") no-repeat scroll center center transparent
      height: 4px
      padding: 11px 5px
      width: 7px
      cursor: pointer
    button#submit.nodropdown
      margin-left: 3px


.paggination a, .paggination span.current, .paggination span.disabled
  border: 1px solid #ccc
  -webkit-border-radius: 3px
  -moz-border-radius: 3px
  border-radius: 3px
  padding: 3px 6px
  margin: 0 1px

.paggination a.active, .paggination span.current
  background: none
  border: none
  color: #434343

#pagination-wrapper
  font-size: 10px
  clear: both
  .paggination
    font-size: 10px
    font-weight: bold
    padding-bottom: 11px
    float: left
    display: inline
    margin-left: 5px
    a:hover
      color: #fff
      background: #ec8526
      border: 1px solid #d77011
  #pagination-counter
    display: inline-block
    padding-bottom: 11px
    margin-left: 5px
    float: left
  a.section
    top: -10px
    width: 82px
    margin-bottom: 8px
    position: relative
    float: right


